<!DOCTYPE html>
<html>
<head>
  <% include ../partials/header.ejs %>
</head>

<body>

<% include ../partials/nav.ejs %>

<div class="container">
<h2>Database Results</h2>

<ul>
    <% results.forEach(function(r) { %>
       <li><%= r.id %> - <%= r.fname %> - <%= r.lname %> - <%= r.gender %> - <%= r.skintone %> - <%= r.hcolour %> - <%= r.fcolour %> - <%= r.weight %> - <%= r.wtype %> - <%= r.height %> - <%= r.htype %> - <%= r.gpa %>
        </li>
    <% }); %>
</ul>

</div>

</body>
</html>
